<div id="<%= dom_id product %>">
  <div>
    <strong>Sku:</strong>
    <%= product.sku %>
  </div>

  <div>
    <strong>Name:</strong>
    <%= product.name %>
  </div>

  <div>
    <strong>Stock:</strong>
    <%= product.stock %>
  </div>

  <div>
    <strong>Stock status:</strong>
    <%= product.stock_status %>
  </div>

</div>
